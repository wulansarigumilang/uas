#include <iostream>
using namespace std;

int main()
{
	int angka[10];
	int x, indeks;
	
	for (int i=0; i<10; i++)
	{
		cout<<"Indeks ke-"<<i+0<<" = "; cin>>angka[i];	
	}
	
	cout<<"\nMasukkan angka yang dicari = "; cin>>x;
	
	for (int i=0; i<10; i++)
	{
		if (angka[i]==x)
		{
			cout<<"\nTerdapat angka "<<x<<" pada indeks ke "<<i;
		}
		
		else if (angka[i]<x)
		{
			cout<<"\nAngka "<<x<<" tidak ditemukan";
			break;
		}
		
	}
		
	return 0;
}
